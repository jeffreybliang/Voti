{
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current" // Use the current version of Node.js instead of specifying a fixed version like 10
          },
          "useBuiltIns": "usage", // Only includes necessary polyfills
          "corejs": 3 // Latest version of core-js
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties", // For class properties
    ]
  }
  