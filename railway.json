{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
      "builder": "NIXPACKS"
  },
  "deploy": {
      "startCommand": "cd ./wh100/backend && gunicorn backend.wsgi:application --bind 0.0.0.0:8000",
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 5
  }
}
